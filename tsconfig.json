{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "vscpp",
    "jsxFragmentFactory": "vscppf",
    "lib": ["esnext"],
    "module": "CommonJS",
    "outDir": "out",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "useUnknownInCatchVariables": false
  },
  "exclude": ["node_modules", ".vscode-test"],
  "include": ["src"]
}
